{
  "name": "@mep-agency/next-iubenda",
  "version": "1.0.0-alpha3",
  "private": false,
  "description": "A React library for integrating Iubenda's Cookie Solution into Next.js projects",
  "source": "src/index.tsx",
  "main": "src/index.tsx",
  "sideEffects": false,
  "scripts": {
    "build": "yarn sass --no-source-map src/components/ConsentAwareWrapper/styles.module.scss src/components/ConsentAwareWrapper/styles.module.css && prettier --write src/components/ConsentAwareWrapper/styles.module.css",
    "watch": "yarn sass --watch --no-source-map src/components/ConsentAwareWrapper/styles.module.scss src/components/ConsentAwareWrapper/styles.module.css",
    "lint": "eslint \"**/*.{ts,tsx,js}\" && prettier --check \"**/*.{ts,tsx,md,scss,css,js}\"",
    "format": "prettier --write \"**/*.{ts,tsx,md,scss,css,js}\""
  },
  "files": [
    "src",
    "LICENCE",
    "README.md"
  ],
  "exports": {
    ".": "./src/index.tsx"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mep-agency/next-iubenda.git"
  },
  "keywords": [
    "next",
    "next.js",
    "react",
    "privacy policy",
    "cookie policy",
    "gdpr",
    "consent management",
    "iubenda"
  ],
  "author": "Marco Lipparini <developer@liarco.net>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mep-agency/next-iubenda/issues"
  },
  "peerDependencies": {
    "next": ">= 13.0.0",
    "react": ">= 18.2.0"
  },
  "devDependencies": {
    "@types/react": "^18.0.21",
    "eslint": "8.43.0",
    "eslint-config-next": "^13.4.7",
    "eslint-config-prettier": "^8.8.0",
    "next": "^13.4.7",
    "prettier": "^2.8.8",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "sass": "^1.63.6",
    "typescript": "^5.1.3"
  },
  "dependencies": {}
}
